{
    "compilerOptions": {
      "alwaysStrict": true,
      "baseUrl": ".",
      "emitDecoratorMetadata": true,
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "forceConsistentCasingInFileNames": true,
      "importHelpers": true,
      "importsNotUsedAsValues": "error",
      "lib": ["es2020"],
      "module": "commonjs",
      "moduleResolution": "node",
      "newLine": "lf",
      "noEmitHelpers": true,
      "noImplicitAny": true,
      "noImplicitThis": true,
      "noImplicitOverride": true,
      "outDir": "dist",
      "preserveConstEnums": true,
      "pretty": true,
      "removeComments": true,
      "resolveJsonModule": true,
      "rootDir": "src",
      "skipLibCheck": true,
      "sourceMap": true,
      "strict": true,
      "strictNullChecks": true,
      "strictPropertyInitialization": true,
      "target": "es2020"
    },
    "typedocOptions": {
      "entryPoints": ["src/docs.ts"],
      "out": "docs",
      "name": "YulxVol Manager Documentation",
      "json": "docs/docs.json",
      "excludePrivate": true,
      "excludeInternal": true,
      "includeVersion": true,
      "disableSources": true,
      "readme": "none",
      "defaultCategory": "Misc",
      "hideGenerator": true,
      "theme": "default",
      "emit": true,
    }
}
  