{
  "name": "poro-bot",
  "version": "1.0.0",
  "description": "Discord bot for managing the Poro Group",
  "main": "packages/index.js",
  "repository": "https://github.com/RediPanda/PoroBot.git",
  "author": "RediPanda <redi@nekonii.xyz>",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "prod": "cross-env NODE_ENV=production node dist/index.js",
    "prod:fix": "cross-env NODE_ENV=production ts-node src/index.ts",
    "prod:test": "cross-env NODE_ENV=production ts-node dist/index.js",
    "build": "npx tsc",
    "flush": "cross-env NODE_ENV=development STATE=FLUSH npx ts-node src/index.ts",
    "dev": "cross-env NODE_ENV=development nodemon --watch src -e ts,json --ignore src/storage --delay 8 src/index.ts",
    "lint": "eslint --cache src",
    "docs": "npx typedoc --tsconfig tsconfig.json",
    "slash": "ts-node ./src/Framework/WS/DeploySlashCommands.ts"
  },
  "dependencies": {
    "@discordjs/builders": "^0.15.0",
    "@discordjs/rest": "^0.1.0-canary.0",
    "@sentry/node": "^6.13.2",
    "@sentry/tracing": "^6.13.2",
    "@types/better-sqlite3": "^7.4.2",
    "@types/node": "^16.10.1",
    "@types/randomatic": "^3.1.3",
    "@types/xml2js": "^0.4.9",
    "better-sqlite3": "^7.4.6",
    "bufferutil": "^4.0.6",
    "canvas": "^2.8.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.7",
    "discord-akairo": "^8.1.0",
    "discord-api-types": "^0.23.1",
    "discord-modals": "^1.3.4",
    "discord.js": "13.4.0",
    "dotenv": "^10.0.0",
    "enmap": "^5.8.7",
    "fast-xml-parser": "^4.0.1",
    "ms": "^2.1.3",
    "mysql2": "^2.3.3",
    "node-gyp": "^8.2.0",
    "nodemon": "^2.0.12",
    "randomatic": "^3.1.1",
    "ts-node": "^10.2.1",
    "tsc": "^2.0.3",
    "typedoc": "^0.22.4",
    "typescript": "^4.4.3",
    "utf-8-validate": "^5.0.9",
    "xml2js": "^0.4.23",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@types/ms": "^0.7.31",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "eslint": "^7.32.0"
  }
}
